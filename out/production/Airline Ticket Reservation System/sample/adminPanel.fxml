                </Button>
                <Button layoutX="10.0" layoutY="105.0" mnemonicParsing="false" onAction="#deleteButtonAction" prefHeight="44.0" prefWidth="250.0" stylesheets="@Style.css" text="Delete" textFill="#fcf8f8">
