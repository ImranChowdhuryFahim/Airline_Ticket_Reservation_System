                    <font>
                        <Font size="30.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" onAction="#addFlightButtonAction" prefHeight="44.0" prefWidth="250.0" stylesheets="@Style.css" text="ADD Flight" textFill="#fcf8f8">
                    <font>
                        <Font name="Ebrima Bold" size="18.0" />
                    </font>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
                </Button>
                <Button layoutX="10.0" layoutY="61.0" mnemonicParsing="false" onAction="#updateButtonAction" prefHeight="44.0" prefWidth="250.0" stylesheets="@Style.css" text="UPDATE" textFill="#fcf8f8">
